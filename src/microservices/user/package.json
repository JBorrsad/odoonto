{
  "name": "@odoonto/user-service",
  "version": "1.0.0",
  "description": "User microservice for Odoonto dental application",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "migrate": "ts-node database/migrator.ts",
    "seed": "ts-node -e \"require('./database/migrator.ts').seedUserData()\""
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/microservices": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "amqp-connection-manager": "^4.1.0",
    "amqplib": "^0.10.0",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.14.0",
    "nestjs-slonik": "^9.0.0",
    "oxide.ts": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "slonik": "^30.4.4",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@slonik/migrator": "^0.11.0",
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
